import java.util.Locale;
import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		Locale.setDefault(Locale.US);
		Scanner sc = new Scanner(System.in);
	
		String nome;
		double a, b, total;
		
		nome = sc.next();
		a = sc.nextDouble();
		b = sc.nextDouble();
				
		total = a + b * 0.15;
		
		
		System.out.printf("TOTAL =  R$ %.2f\n", total);
		
		sc.close();
		
	}

}
