import java.util.Locale;
import java.util.Scanner;
public class Main {

	public static void main(String[] args) {
	Locale.setDefault(Locale.US);
	Scanner sc = new Scanner(System.in);

	int km, tempo;

    km = sc.nextInt();
    
    tempo = 2 * km;

    System.out.println(tempo + " Minutos");
	
	sc.close();
	}
}
